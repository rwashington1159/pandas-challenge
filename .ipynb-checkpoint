    "all_schools = df.groupby([\"school_name\"]).mean()\n",
    "all_schools[\"Student Per Budget\"] = all_schools[\"budget\"]/all_schools[\"size\"]\n",
    "all_schools[\"% Passing Math\"] = round(all_schools[\"Average Math Score\"]*100,2)\n",
    "all_schools[\"% Passing Reading\"]= round(all_schools[\"Average Reading Score\"]*100, 2)\n",
    "all_schools[\"Overall Passing\"] = round(((all_schools[\"Average Math Score\"] + all_schools[\"Average Math Score\"])/2)*100,0)\n",
    "all_schools"
   ]
