    "district_summary['Avg Reading Score'] = district_summary['Avg Reading Score'].map(\"{:.1f}\".format)\n",
    "district_summary['Percent Passing Math'] = district_summary['Percent Passing Math'].map(\"{:.1f}%\".format)\n",
    "district_summary['Percent Passing Reading'] = district_summary['Percent Passing Reading'].map(\"{:.1f}%\".format)\n",
    "district_summary['Overall Passing'] = district_summary['Overall Passing'].map(\"{:.1f}%\".format)\n",
    "\n",
