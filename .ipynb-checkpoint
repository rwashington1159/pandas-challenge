    "#Performance summary \n",
    "performance_summary = pd.merge(all_schools, school_data, how=\"left\", on=[\"school_name\", \"school_name\"])\n",
    "performance_summary \n",
    "performance_summary.drop([\"Student ID\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"School ID_x\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"School ID_y\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"size_y\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"budget_y\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"reading_score\"], axis = 1, inplace = True)\n",
    "performance_summary.drop([\"math_score\"], axis = 1, inplace = True)\n",
    "\n",
    "performance_summary \n"
